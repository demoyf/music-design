music-color = #31c27c;
my-min-width = 1100px;
inpur-second = .75s;//输入框的动画时长
roll-scond = 1.5s;//滚动的动画时长
scale-second = .5s;//缩放的时长
max-screen-width = 82%;
min-screen-width=90%;
border-1px($color)
  position: relative
  &:after
    display: block
    position: absolute
    left: 0
    bottom: 0
    width: 100%
    border-top: 1px solid $color
    content: ' '
add_prefix(prop,arg)
	-webkit-{prop}: arg;
	-ms-{prop}: arg;
	-o-{prop}: arg;
	{prop}: arg;

ellipsis_tran()
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;

my-font-stretch()
  text-align: center;
  margin-bottom: 1.8em;
  font-size: 2em;
  color:#333;
  letter-spacing:.3em;
  font-stretch:narrower;
  font-family:SimHei;
  font-weight:bold;
  -webkit-transform: scale(.95);
  transform: scale(.95);
