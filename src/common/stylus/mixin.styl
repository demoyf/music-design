music-color = #31c27c
my-min-width = 1100px;
border-1px($color)
  position: relative
  &:after
    display: block
    position: absolute
    left: 0
    bottom: 0
    width: 100%
    border-top: 1px solid $color
    content: ' '
add_prefix(prop,arg)
	-webkit-{prop}: arg;
	-ms-{prop}: arg;
	-o-{prop}: arg;
	{prop}: arg;

ellipsis_tran()
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;  